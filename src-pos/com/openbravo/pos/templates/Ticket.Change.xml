import com.openbravo.pos.payment.PaymentInfo;
import javax.swing.JOptionPane;

boolean isCash = false;
String change = "";
PaymentInfo p = ticket.payments.getFirst();

if ("cash".equals(p.getName())) {
    isCash = true;
    change = p.printChange();
}
if(isCash) {
    javax.swing.JOptionPane.showMessageDialog(null, "El cambio es de:             " + change + "             pesos.", "Mensaje de PeoniaPOS.", JOptionPane.INFORMATION_MESSAGE);
    return "Cancel";
}